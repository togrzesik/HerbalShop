@model IEnumerable<Sklep.Models.ViewModel.DeliveryView>
@{
    ViewBag.Title = "Koszty dostawy";
    Layout = "~/Views/Shared/LayoutNotLogged.cshtml";
}
<title>Koszty dostawy</title>
<div class="container containerStore container--delivery">
    <p class="delivery_title">Dostawa</p>
    <div class="row">
        <div class="col-12 col-md-8 mx-auto">
            <table id="example" class=" mdl-data-table mdl-js-data-table table-responsive delivery_table" width="100%">
                <thead>
                    <tr>
                        <th class="mdl-data-table__cell--non-numeric col-md-5 ">Przesyłka</th>
                        <th class="mdl-data-table__cell--non-numeric width-50">Cena</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td class="mdl-data-table__cell--non-numeric col-md-5 text-wrap">@item.DeliveryName</td>
                            <td class="mdl-data-table__cell--non-numeric width-50">@item.DeliveryPriceB</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>